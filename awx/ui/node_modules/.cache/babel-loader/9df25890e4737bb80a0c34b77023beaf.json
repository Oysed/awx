{"ast":null,"code":"export default function getRowRangePageSize(startIndex,stopIndex){var page;var pageSize;if(startIndex===stopIndex){page=startIndex;pageSize=1;}else if(stopIndex>=startIndex+50){page=Math.floor(startIndex/50)+1;pageSize=50;}else{for(var i=stopIndex-startIndex+1;i<=50;i++){if(Math.floor(startIndex/i)===Math.floor(stopIndex/i)||i===50){page=Math.floor(startIndex/i)+1;pageSize=i;break;}}}return{page:page,pageSize:pageSize,firstIndex:(page-1)*pageSize};}","map":{"version":3,"names":["getRowRangePageSize","startIndex","stopIndex","page","pageSize","Math","floor","i","firstIndex"],"sources":["/awx_devel/awx/ui/src/screens/Job/JobOutput/shared/jobOutputUtils.js"],"sourcesContent":["export default function getRowRangePageSize(startIndex, stopIndex) {\n  let page;\n  let pageSize;\n\n  if (startIndex === stopIndex) {\n    page = startIndex;\n    pageSize = 1;\n  } else if (stopIndex >= startIndex + 50) {\n    page = Math.floor(startIndex / 50) + 1;\n    pageSize = 50;\n  } else {\n    for (let i = stopIndex - startIndex + 1; i <= 50; i++) {\n      if (\n        Math.floor(startIndex / i) === Math.floor(stopIndex / i) ||\n        i === 50\n      ) {\n        page = Math.floor(startIndex / i) + 1;\n        pageSize = i;\n        break;\n      }\n    }\n  }\n\n  return {\n    page,\n    pageSize,\n    firstIndex: (page - 1) * pageSize,\n  };\n}\n"],"mappings":"AAAA,cAAe,SAAS,CAAAA,mBAAmBA,CAACC,UAAU,CAAEC,SAAS,CAAE,CACjE,GAAI,CAAAC,IAAI,CACR,GAAI,CAAAC,QAAQ,CAEZ,GAAIH,UAAU,GAAKC,SAAS,CAAE,CAC5BC,IAAI,CAAGF,UAAU,CACjBG,QAAQ,CAAG,CAAC,CACd,CAAC,IAAM,IAAIF,SAAS,EAAID,UAAU,CAAG,EAAE,CAAE,CACvCE,IAAI,CAAGE,IAAI,CAACC,KAAK,CAACL,UAAU,CAAG,EAAE,CAAC,CAAG,CAAC,CACtCG,QAAQ,CAAG,EAAE,CACf,CAAC,IAAM,CACL,IAAK,GAAI,CAAAG,CAAC,CAAGL,SAAS,CAAGD,UAAU,CAAG,CAAC,CAAEM,CAAC,EAAI,EAAE,CAAEA,CAAC,EAAE,CAAE,CACrD,GACEF,IAAI,CAACC,KAAK,CAACL,UAAU,CAAGM,CAAC,CAAC,GAAKF,IAAI,CAACC,KAAK,CAACJ,SAAS,CAAGK,CAAC,CAAC,EACxDA,CAAC,GAAK,EAAE,CACR,CACAJ,IAAI,CAAGE,IAAI,CAACC,KAAK,CAACL,UAAU,